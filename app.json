{
  "expo": {
    "name": "OnFocus",
    "slug": "OnFocus",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/Img/Logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ff1"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptiveIcon.png",
        "backgroundColor": "#fff10f"
      },
      "config": {
        "googleSignIn": {
          "apiKey": "YOUR_API_KEY",
          "certificateHash": "YOUR_CERTIFICATE_HASH"
        }
      },
      "package": "com.exemple.onfocus",
      "permissions": ["CAMERA", "LOCATION"],
      "versionCode": 1,
      "versionName": "1.0",
      "softwareKeyboardLayoutMode": "pan"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
